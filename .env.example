# Redis настройки
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_EXTERNAL_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# OCR настройки
OCR_USE_GPU=False

# LangChain и LLM API ключи
OPENAI_API_KEY=sk-your-openai-key-here
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# LLM модели
PRIMARY_LLM_MODEL=gpt-4o
FALLBACK_LLM_MODEL=claude-3-5-sonnet-20241022
LLM_TEMPERATURE=0

# Synonym matching настройки
SYNONYM_DB_PATH=equipment_synonyms.json
SYNONYM_MATCH_METHOD=hybrid
SYNONYM_CONFIDENCE_THRESHOLD=0.8

# Semantic model
SEMANTIC_MODEL_NAME=paraphrase-multilingual-MiniLM-L12-v2

# Qdrant настройки (векторная база для semantic search)
QDRANT_HOST=localhost
QDRANT_HTTP_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_COLLECTION_NAME=equipment_synonyms

# Extraction настройки
EXTRACTION_MAX_TEXT_LENGTH=15000
EXTRACTION_MAX_RETRIES=3

# PDF классификация
IMAGE_COVERAGE_THRESHOLD=0.5
TEXT_COVERAGE_THRESHOLD=0.1

# Worker настройки
MAX_RETRIES=3
RETRY_DELAY=5
TASK_TIMEOUT=600

# Директории
TEMP_DIR=./temp
OUTPUT_DIR=./output
